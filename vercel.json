{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/form",
      "dest": "/form.html"
    },
    {
      "src": "/coming-soon",
      "dest": "/coming-soon.html"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
